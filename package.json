{
  "name": "mos",
  "version": "0.0.0-placeholder",
  "description": "A pluggable module that injects content into your markdown files via hidden JavaScript snippets",
  "files": [
    "index.js",
    "lib",
    "plugins"
  ],
  "bin": "./lib/cli.js",
  "engines": {
    "node": ">=4"
  },
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "md:test": "node ./lib/cli test -x=\"lib/test-cli/**\"",
    "lint": "eslint lib/**/*.js plugins/**/*.js index.js",
    "test:code": "mocha",
    "test": "npm run test:code && npm run lint && npm run md:test",
    "coverage": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha -- -R spec",
    "precoveralls": "istanbul cover -x \"**/*.spec.js\" node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage",
    "md": "node ./lib/cli -x=\"lib/test-cli/**\"",
    "prepublish": "npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zkochan/mos"
  },
  "keywords": [
    "markdown",
    "mdown",
    "mkdn",
    "mkd",
    "md",
    "readme",
    "template",
    "generator",
    "documentation",
    "badges",
    "shields",
    "example",
    "test"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/mos/issues"
  },
  "homepage": "https://github.com/zkochan/mos#readme",
  "dependencies": {
    "chalk": "~1.1.3",
    "file-exists": "~1.0.0",
    "github-url-to-object": "~2.2.1",
    "glob": "~7.0.3",
    "meow": "~3.7.0",
    "mos-plugin-dependencies": "^1.0.1",
    "mos-plugin-example": "^0.1.0",
    "mos-plugin-shields": "^1.0.0",
    "mos-plugin-snippet": "^1.0.0",
    "normalize-newline": "~2.0.0",
    "normalize-path": "~2.0.1",
    "read-pkg-up": "~1.0.1",
    "relative": "~3.0.2",
    "remark": "~4.2.2",
    "remark-mos": "^1.2.0",
    "remark-toc": "~3.0.0",
    "resolve": "~1.1.7",
    "run-async": "~2.2.0",
    "tap-diff": "~0.1.1",
    "tape": "~4.5.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "execa": "^0.4.0",
    "ghooks": "^1.2.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "semantic-release": "^4.3.5",
    "validate-commit-msg": "^2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js",
      "pre-commit": "npm test"
    }
  }
}
